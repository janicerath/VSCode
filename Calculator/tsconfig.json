{
  "compilerOptions": {
    "target": "es6",                          
    "module": "commonjs", 
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,   
    "sourceMap": false,  
    "removeComments": false, 
    "outDir": "JSFiles",    
    "esModuleInterop": true ,
    "types": ["jasmine", "node"]
  },
  "exclude": [
    "node_modules"
  ]
}
